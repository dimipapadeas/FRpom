<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.example</groupId>
    <artifactId>ForgeRock-Build-Deploy</artifactId>
    <packaging>pom</packaging>
    <version>1</version>
    <name>ForgeRock-build-deploy</name>
    <modules>
        <module>forgerock-parent-public</module>
		<module>forgerock-build-tools-public</module>
    </modules>
    <build>
        <extensions>
            <extension>
                <groupId>org.apache.maven.wagon</groupId>
                <artifactId>wagon-webdav-jackrabbit</artifactId>
                <version>3.0.0</version>
            </extension>
        </extensions>
    </build>
<distributionManagement>
		<repository>
			<id>openwis-release</id>
			<name>OpenWIS Release Repository</name>
			<url>http://repository-openwis-association.forge.cloudbees.com/release</url>
		</repository>
		<snapshotRepository>
			<id>openwis-snapshot</id>
			<name>OpenWIS Snapshot Repository</name>
			<url>http://repository-openwis-association.forge.cloudbees.com/snapshot</url>
		</snapshotRepository>
	</distributionManagement>  
</project>